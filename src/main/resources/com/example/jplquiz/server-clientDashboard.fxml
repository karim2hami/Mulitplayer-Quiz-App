<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns="http://javafx.com/javafx"
  xmlns:fx="http://javafx.com/fxml"
  fx:controller="com.example.jplquiz.controller.ServerClientDashboard"
  prefHeight="400.0" prefWidth="600.0">
  <children>
    <BorderPane prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #612CB9;">
      <center>
        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
          <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="417.0">
              <children>
                <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="60.0" prefWidth="110.0">
                  <children>
                    <Pane prefHeight="160.0" prefWidth="200.0" style="-fx-background-color: #31036C; -fx-background-radius: 5;" />
                    <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="69.0" prefWidth="108.0">
                      <children>
                        <ImageView fitHeight="37.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                            <Image url="@../../../icons/Person.png" />
                          </image></ImageView>
                        <Label fx:id="lb_playerCounter" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="49.0" prefWidth="49.0" text="20" textFill="WHITE">
                          <font>
                            <Font size="32.0" />
                          </font>
                          <HBox.margin>
                            <Insets left="20.0" />
                          </HBox.margin>
                          <padding>
                            <Insets top="10.0" />
                          </padding></Label>
                      </children>
                      <StackPane.margin>
                        <Insets bottom="10.0" />
                      </StackPane.margin>
                    </HBox>
                  </children>
                </StackPane>
                <StackPane prefHeight="150.0" prefWidth="200.0">
                  <children>
                    <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="60.0" prefWidth="300.0" style="-fx-background-color: #31036C #31036C; -fx-background-radius: 5;">
                      <StackPane.margin>
                        <Insets />
                      </StackPane.margin>
                    </Pane>
                    <Label text="Waiting for Players to join..." textFill="WHITE">
                      <font>
                        <Font size="24.0" />
                      </font></Label>
                  </children>
                  <HBox.margin>
                    <Insets left="150.0" right="150.0" />
                  </HBox.margin>
                </StackPane>
                <Button mnemonicParsing="false" style="-fx-background-color: #E1E1E1;" text="Start">
                  <font>
                    <Font size="24.0" />
                  </font></Button>
              </children>
              <VBox.margin>
                <Insets left="30.0" right="30.0" />
              </VBox.margin>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" />
          </children>
        </VBox>
      </center>
      <top>
        <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
          <children>
            <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="500.0" style="-fx-background-color: #FFFF; -fx-background-radius: 5;" />
            <Label text="Quiz Player Dashboard">
              <font>
                <Font name="System Bold" size="36.0" />
              </font>
            </Label>
          </children>
        </StackPane>
      </top>
    </BorderPane>
  </children>

</AnchorPane>
